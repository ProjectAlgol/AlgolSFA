<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@android:id/background">
        <shape>
            <corners android:radius="5dip" />
            <gradient
                android:startColor="#ffF5F5F5"
                android:centerColor="#ffD3D3D3"
                android:centerY="0.75"
                android:endColor="#ffDCDCDC"
                android:angle="270"
                />
        </shape>
    </item>

    <item android:id="@android:id/secondaryProgress">
        <clip>
            <shape>
                <corners android:radius="5dip" />
                <gradient
                    android:startColor="#809d9e9d"
                    android:centerColor="#805a5d5a"
                    android:centerY="0.75"
                    android:endColor="#a0747674"
                    android:angle="270"
                    />
            </shape>
        </clip>
    </item>
    <item
        android:id="@android:id/progress"
        >
        <clip>
            <shape>
                <corners
                    android:radius="5dip" />
                <gradient
                    android:startColor="#9d9e9d"
                    android:endColor="#8a8d8a"
                    android:angle="270" />
            </shape>
        </clip>
    </item>

</layer-list>